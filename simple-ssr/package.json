{
  "private": true,
  "name": "client",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:build:*\"",
    "dev:server": "node dist/server/main.js",
    "dev:build:client": "webpack --config ./webpack.client.config.js --watch --mode development",
    "dev:build:server": "webpack --config ./webpack.server.config.js --watch --mode development"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.16",
    "react-router": "^6.3.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
